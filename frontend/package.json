{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://127.0.0.1:5000",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.30.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.38.1",
    "react-responsive": "^9.0.0-beta.8",
    "react-router-dom": "6",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "wavesurfer.js": "^6.1.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "tailwind:build": "tailwind build -i ./src/index.tailwind.css -c ./tailwind.config.js -o ./src/index.css",
    "tailwind:watch": "tailwind build -i ./src/index.tailwind.css -c ./tailwind.config.js -o ./src/index.css --watch",
    "prestart": "yarn tailwind:build",
    "start:dev": "concurrently \"react-scripts start\" \"yarn tailwind:watch\"",
    "start": "concurrently \"react-scripts start\" \"yarn tailwind:build\"",
    "prebuild": "yarn tailwind:build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@hookform/devtools": "^4.1.0",
    "@types/react-icons": "^3.0.0",
    "@types/wavesurfer.js": "^6.0.3",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.1.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24"
  }
}